{
  "name": "nightkay-app-boilerplate",
  "version": "0.1.0",
  "description": "nightkay-app-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "_build": "BABEL_ENV=production webpack --progress --env=production",
    "build": "npm run _build && npm run tar",
    "tar": "tar zcvf build.tar.gz build",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": "zenxds"
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
